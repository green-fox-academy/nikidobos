@model BankOfSimba.ViewModels.BankViewModel
@{
    ViewData["Title"] = "GetBankAccounts";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>GetBankAccounts</h2>

@foreach (var item in Model.accounts)
{
    <p>@item.Name</p>
    <p>@item.Balance.ToString("#.00 Zebra")</p>
    <p>@item.AnimalType</p>

}

